// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(uuid())
  email     String @unique
  password  String
  firstName String
  lastName  String
  // Add more user-related fields as needed

  // // Define a relation to bookings
  // bookings  Booking[]
  // // Define a relation to reviews
  // reviews   Review[]
}
